<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:IOTGatewayHost.Styles.ThemeA">
    <Color x:Key="AccentColor">#FF9F39</Color>
    <Color x:Key="ForegroundColor">White</Color>
    <SolidColorBrush x:Key="AccentColorBrush" Color="{StaticResource AccentColor}" />
    <SolidColorBrush x:Key="ForegroundColorBrush" Color="{StaticResource ForegroundColor}" />
    <LinearGradientBrush x:Key="BackgroundBrush" EndPoint="0,1.5" StartPoint="1.5,1.5">
        <GradientStop Color="#312D3D" Offset="0"/>
        <GradientStop Color="#2E2A3A" Offset="0"/>
    </LinearGradientBrush>
</ResourceDictionary>